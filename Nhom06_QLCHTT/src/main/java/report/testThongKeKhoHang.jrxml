	<queryString language="SQL">
		<![CDATA[SELECT "SanPham"."maSP",
	"SanPham"."tenSP",
	"DanhMuc"."tenDM",
	"MauSac"."tenMS",
	"ChatLieu"."tenCL",
	"KichCo"."tenKC",
	"NhaCungCap"."tenNCC",
	"SanPham"."donGia",
	COALESCE( sum( ChiTietHoaDon.soLuong),0) AS soLuongDaBan,
	"SanPham"."soLuongTon"
FROM "HoaDon"
	inner join "ChiTietHoaDon" ON 
	 "HoaDon"."maHD" = "ChiTietHoaDon"."maHD" 
	full join "SanPham" ON 
	 "ChiTietHoaDon"."maSP" = "SanPham"."maSP" 
	inner join "DanhMuc" ON 
	 "SanPham"."maDM" = "DanhMuc"."maDM" 
	inner join "MauSac" ON 
	 "SanPham"."maMS" = "MauSac"."maMS" 
	inner join "ChatLieu" ON 
	 "SanPham"."maCL" = "ChatLieu"."maCL" 
	inner join "KichCo" ON 
	 "SanPham"."maKC" = "KichCo"."maKC" 
	inner join "NhaCungCap" ON 
	 "SanPham"."maNCC" = "NhaCungCap"."maNCC" 
GROUP BY "SanPham"."maSP",
	"SanPham"."tenSP",
	"DanhMuc"."tenDM",
	"MauSac"."tenMS",
	"ChatLieu"."tenCL",
	"KichCo"."tenKC",
	"NhaCungCap"."tenNCC",
	"SanPham"."donGia",
	"SanPham"."soLuongTon"
ORDER BY "SanPham"."soLuongTon" DESC]]>
	</queryString>
